{
    "name": "mitsuru793/list-project-sorting-by-modified",
    "description": "List project sorting by modified.",
    "type": "library",
    "homepage": "https://github.com/mitsuru793/php-list-project-sroting-by-modified",
    "license": "MIT",
    "keywords": [
        "git",
        "list",
        "ls",
        "modified",
        "sort"
    ],
    "authors": [
        {
            "name": "mitsuru793",
            "email": "mitsuru793@gmail.com",
            "role": "Developer"
        }
    ],
    "bin": ["bin/list-project-sorting-by-modified"],
